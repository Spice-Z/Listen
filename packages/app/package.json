{
  "name": "listen-app",
  "private": true,
  "scripts": {
    "start": "expo start --dev-client",
    "start:production": "NODE_ENV=production expo start --no-dev --minify",
    "lint:fix": "eslint --fix '**/*.{ts,tsx}'",
    "lint": "eslint '**/*.{ts,tsx}'",
    "run:android": "react-native run-android",
    "run:ios": "react-native run-ios",
    "build:local:android-preview": "APP_VARIANT=development eas build --platform android --profile preview --local",
    "build:local:ios": "eas build --profile development --platform ios --local",
    "build:prod:ios": "APP_VARIANT=production NODE_ENV=production eas build --profile production --platform ios",
    "build:prod:ios-local": "APP_VARIANT=production NODE_ENV=production eas build --profile production --platform ios --local",
    "build:prod:android": "eas build --profile production --platform android",
    "build:local:ios-preview": "eas build --profile preview --platform ios --local",
    "submit:ios": "APP_VARIANT=production NODE_ENV=production && eas submit --platform ios ",
    "submit:android": "eas submit --platform android",
    "eas:device": "eas device:list",
    "eas:devive:create": "eas device:create",
    "eas:credentials": "eas credentials",
    "eas:secret:list": "eas secret:list",
    "android": "expo run:android",
    "expo:run:ios": "APP_VARIANT=development expo run:ios",
    "expo:run:ios:prod": "APP_VARIANT=production expo run:ios",
    "gql:generate": "graphql-codegen",
    "test": "vitest --config vitest.config.ts",
    "ios": "expo run:ios"
  },
  "dependencies": {
    "@apollo/client": "^3.8.1",
    "@babel/runtime": "^7.21.0",
    "@invertase/react-native-apple-authentication": "^2.2.2",
    "@react-native-community/slider": "4.4.2",
    "@react-native-firebase/analytics": "^18.3.2",
    "@react-native-firebase/app": "^18.3.2",
    "@react-native-firebase/auth": "^18.3.2",
    "@react-native-firebase/crashlytics": "^18.3.2",
    "@react-native-firebase/functions": "^18.3.2",
    "@react-native-firebase/remote-config": "^18.3.2",
    "@react-native-google-signin/google-signin": "^10.0.1",
    "@tanstack/react-query": "^4.28.0",
    "@types/react": "~18.0.27",
    "axios": "^1.3.5",
    "date-fns": "^2.29.3",
    "expo": "49.0.7",
    "expo-application": "^5.3.0",
    "expo-build-properties": "~0.5.1",
    "expo-constants": "~14.4.2",
    "expo-dev-client": "~2.1.6",
    "expo-image": "~1.3.2",
    "expo-linear-gradient": "~12.1.2",
    "expo-linking": "~4.0.1",
    "expo-router": "^2.0.2",
    "expo-screen-orientation": "~5.1.1",
    "expo-splash-screen": "~0.18.2",
    "expo-status-bar": "~1.4.2",
    "graphql": "^16.8.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-native": "0.72.4",
    "react-native-gesture-handler": "~2.9.0",
    "react-native-google-mobile-ads": "^12.1.1",
    "react-native-reanimated": "~3.4.2",
    "react-native-safe-area-context": "4.5.0",
    "react-native-screens": "~3.20.0",
    "react-native-svg": "13.4.0",
    "react-native-track-player": "^3.2.0",
    "react-native-web": "~0.18.7",
    "react-native-webview": "13.2.2",
    "expo-tracking-transparency": "~3.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.19.3",
    "@graphql-codegen/cli": "^5.0.0",
    "@graphql-codegen/client-preset": "^4.1.0",
    "@types/react-native": "^0.72.2",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-native": "^4.0.0",
    "vite": "^4.4.9",
    "vitest": "^0.34.4"
  },
  "version": "1.0.0"
}
