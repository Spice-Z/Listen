{
  "name": "listen-app",
  "private": true,
  "scripts": {
    "start": "NODE_ENV=development APP_VARIANT=development expo start --dev-client",
    "start:production": "NODE_ENV=production expo start --no-dev --minify",
    "lint:fix": "eslint --fix '**/*.{ts,tsx}'",
    "lint": "eslint '**/*.{ts,tsx}'",
    "build:development:android": "APP_VARIANT=development eas build --profile development --platform android",
    "build:development:ios": "eas build --profile development --platform ios --local",
    "build:prod:ios:eas": "APP_VARIANT=production NODE_ENV=production eas build --profile production --platform ios",
    "build:prod:ios:local": "APP_VARIANT=production NODE_ENV=production eas build --profile production --platform ios --local",
    "build:prod:android:eas": "APP_VARIANT=production NODE_ENV=production eas build --profile production --platform android",
    "build:prod:android:local": "APP_VARIANT=production NODE_ENV=production eas build --profile production --platform android --local",
    "build:preview:ios": "eas build --profile preview --platform ios --local",
    "build:preview:android": "eas build --profile preview --platform android --local",
    "build:preview:ios-prod": "eas build --profile preview-prod --platform ios --local",
    "build:preview:android-prod": "eas build --profile preview-prod --platform android --local",
    "eas:submit:ios": "APP_VARIANT=production NODE_ENV=production && eas submit --platform ios ",
    "eas:submit:android": "APP_VARIANT=production NODE_ENV=production eas submit --platform android",
    "eas:device": "eas device:list",
    "eas:devive:create": "eas device:create",
    "eas:credentials": "eas credentials",
    "eas:secret:list": "eas secret:list",
    "android": "expo run:android",
    "expo:run:ios": "APP_VARIANT=development expo run:ios",
    "expo:run:ios:prod": "APP_VARIANT=production expo run:ios",
    "gql:generate": "graphql-codegen",
    "test": "vitest --config vitest.config.ts",
    "ios": "expo run:ios"
  },
  "dependencies": {
    "@apollo/client": "^3.8.1",
    "@babel/runtime": "^7.21.0",
    "@gorhom/bottom-sheet": "^4.5.1",
    "@invertase/react-native-apple-authentication": "^2.2.2",
    "@react-native-community/slider": "4.4.2",
    "@react-native-firebase/analytics": "^18.3.2",
    "@react-native-firebase/app": "^18.3.2",
    "@react-native-firebase/auth": "^18.8.0",
    "@react-native-firebase/crashlytics": "^18.3.2",
    "@react-native-firebase/functions": "^18.3.2",
    "@react-native-firebase/remote-config": "^18.3.2",
    "@react-native-google-signin/google-signin": "^10.0.1",
    "@tanstack/react-query": "^4.28.0",
    "@types/react": "~18.0.27",
    "axios": "^1.3.5",
    "date-fns": "^2.29.3",
    "expo": "49.0.13",
    "expo-application": "^5.4.0",
    "expo-build-properties": "~0.11.0",
    "expo-constants": "~14.4.2",
    "expo-dev-client": "2.4.7",
    "expo-device": "~5.4.0",
    "expo-image": "~1.5.1",
    "expo-linear-gradient": "~12.5.0",
    "expo-linking": "~6.0.0",
    "expo-notifications": "~0.27.6",
    "expo-router": "^2.0.9",
    "expo-screen-orientation": "~6.2.0",
    "expo-splash-screen": "~0.22.0",
    "expo-status-bar": "~1.7.1",
    "expo-tracking-transparency": "~3.1.0",
    "graphql": "^16.8.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-native": "0.72.4",
    "react-native-gesture-handler": "~2.9.0",
    "react-native-google-mobile-ads": "^12.1.1",
    "react-native-reanimated": "~3.4.2",
    "react-native-safe-area-context": "4.5.0",
    "react-native-screens": "~3.20.0",
    "react-native-svg": "13.4.0",
    "react-native-track-player": "^3.2.0",
    "react-native-web": "~0.18.7",
    "react-native-webview": "13.2.2",
    "@react-native-community/datetimepicker": "7.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.19.3",
    "@graphql-codegen/cli": "^5.0.0",
    "@graphql-codegen/client-preset": "^4.1.0",
    "@types/react-native": "^0.72.2",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-native": "^4.0.0",
    "vite": "^4.4.9",
    "vitest": "^1.2.2"
  },
  "version": "1.0.0"
}
