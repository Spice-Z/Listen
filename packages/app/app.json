{
  "expo": {
    "name": "Listen",
    "slug": "Listen",
    "scheme": "listen-podcast",
    "version": "1.0.0",
    "jsEngine": "hermes",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "dark",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": ["**/*"],
    "plugins": [
      "@react-native-firebase/app",
      [
        "expo-build-properties",
        {
          "ios": {
            "useFrameworks": "static"
          }
        }
      ],
      [
        "expo-screen-orientation",
        {
          "initialOrientation": "DEFAULT"
        }
      ]
    ],
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "UIBackgroundModes": ["audio"]
      },
      "bundleIdentifier": "com.spicex.Listen",
      "requireFullScreen": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.spicex.Listen"
    },
    "extra": {
      "eas": {
        "projectId": "0b079cfe-10c2-470d-aeac-3242cb2a4790"
      }
    }
  },
  "react-native-google-mobile-ads": {
    "android_app_id": "ca-app-pub-5532826448688500~4365274585",
    "ios_app_id": "ca-app-pub-5532826448688500~5869626627",
    "user_tracking_usage_description": "This identifier will be used to deliver personalized ads to you."
  }
}
